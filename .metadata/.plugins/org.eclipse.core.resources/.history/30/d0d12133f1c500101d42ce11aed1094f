package com.tcs.polymorphism.day2;

class Bank
{
     void doPayment()
     {
    	 System.out.println("you can scan and do the payment");
     }
}

class Gpay extends Bank 
{
	@Override
	void doPayment()
	{
		System.out.println("scan and pay the amount");
	}
	
	void loan()
	{
		System.out.println("give loan to every one");
	}
}




public class RunTimePolymorphism {

	public static void main(String[] args) {
		
		Gpay gpay  = new Gpay();
		
		Payment payment = new Gpay();
	
	}
}
