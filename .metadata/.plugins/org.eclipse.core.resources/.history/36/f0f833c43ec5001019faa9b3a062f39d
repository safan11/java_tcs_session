package com.tcs.oops.day1;

public class Car {

	// attribute
	int kilometerDriven;
	int modelNo;
	String ownerName;
	
	public void showDetails()
	{
		System.out.println("Kilometer Driven :"+ kilometerDriven);
		System.out.println("Model No :"+ modelNo);
		System.out.println("OwnerName :"+ ownerName);
	}
	
	
}
