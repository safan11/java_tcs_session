package com.tcs.constructor.day2;

class Employee
{
	 void Employee()
	{
		System.out.println("employee constrcutor");
	}
	
	void display()
	{
		System.out.println("this is display function");
	}
}



public class DefaultConstructor1 {

	public static void main(String[] args) {
		
		Employee employee = new Employee();// whenever we create the object, constructor is called
	
	}
}
