package com.tcs.polymorphism.day2;

class Bank
{
     void minimumAmount()
     {
    	 System.out.println("");
     }
     
     void loan()
     {
    	 System.out.println("all customer above 750 credit score will get loan");
     }
}

class Gpay extends Bank 
{
	@Override
	void doPayment()
	{
		System.out.println("scan and pay the amount");
	}
	
	
}




public class RunTimePolymorphism {

	public static void main(String[] args) {
		
	//	Gpay gpay  = new Gpay(); // It will call both the parent and child methods
	//	gpay.doPayment();
	//	gpay.loan();
		
		Bank payment = new Gpay();//upcasting
		payment.doPayment();
		payment.loan();
	
	
	}
}
