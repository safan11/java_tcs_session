package com.tcs.basic.day1;

public class Task3 {

	public static void main(String[] args) {
		
		for(int i=1;i<=5;i++)
		{
			// stmt1
			System.out.println("start : i = "+ i);
			
			//stmt2
			for(int j=1;j<=3;j++)
			{
				System.out.println("i :"+i +" j:" + j);
			}
			
			//stmt2
			System.out.println("end : i = "+ i);

		}
		
		
	}
}
