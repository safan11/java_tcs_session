package com.tcs.list.day4;

import java.util.ArrayList;

public class BulkOperation {

	public static void main(String[] args) {

		ArrayList<String> batch1Names = new ArrayList<String>();
		batch1Names.add("a");
		batch1Names.add("b");
		batch1Names.add("c");
		batch1Names.add("d");
		
		System.out.println("batch1 names :"+batch1Names);
		
		ArrayList<String> batch2Names = new ArrayList<String>();
		batch2Names.add("c");
		batch2Names.add("d");
		batch2Names.add("e");
		batch2Names.add("f");
		
		System.out.println("batch2 names :"+batch2Names);
		
		//batch1Names.addAll(batch2Names);
		//System.out.println("after adding batch1 :"+ batch1Names);
		
		//batch1Names.removeAll(batch2Names); // common elements we will removed 
		//System.out.println("batch1 names after remove all :"+ batch1Names);
		
		batch1Names.retainAll(batch2Names);// i will hold the common elements
		System.out.println("after retain all :"+ batch1Names);
		
	}

}
