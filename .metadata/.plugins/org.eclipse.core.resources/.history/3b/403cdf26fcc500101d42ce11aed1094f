package com.tcs.interfacedemo.day2;

interface Payment
{
	 void doPayment();
}

class Gpay implements Payment 
{
	public void doPayment()
	{
		System.out.println("iam doing the payment by gpay");
	}
}

class PhonePay implements Payment 
{

	@Override
	public void doPayment() {
       System.out.println("iam doing the payment by phone pay");		
	}
	 
}


public class LoosleyCoupled {

	public static void main(String[] args) {
		
		Payment payment = new Gpay();
		payment.doPayment();
		
	}
}
